    "def compute_prefix_sum(n):\n",
    "    sum = 0\n",
    "    for x in range(1, n + 1):\n",
    "        sum += x\n",
    "    return sum\n",
    "\n",
    "def main():\n",
    "    count = 20\n",
    "    while count > 0:\n",
    "        print(count)\n",
    "        sum = compute_prefix_sum(count)\n",
    "        print(sum)\n",
    "        count -= 1\n",
    "\n",
    "main()\n",
    "\n"
   ]
  }
